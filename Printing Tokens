#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char ch[1000];
    scanf("%[^\n]%*c",ch);
    

    for(int j=0;j<strlen(ch);j++)
    {
        if(ch[j]!=' '){
         printf("%c",ch[j]);
        }
        
        else{
            printf("\n");
        }
    }



    return 0;
}


